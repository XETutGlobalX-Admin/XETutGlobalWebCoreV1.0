    <div class="">
        <div class="offcanvas offcanvas-start xetut-home-offcanvas-start" data-bs-scroll="true" data-bs-backdrop="true" tabindex="-1" id="offcanvasScrolling" aria-labelledby="offcanvasScrollingLabel">
            <div class="offcanvas-header">
                <h6 class="offcanvas-title d-sm-block" id="offcanvas">Menu</h6>
                <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <div class="offcanvas-body px-0">
                <ul class="nav nav-pills flex-column mb-sm-auto mb-0 align-items-start list-group" id="menu">
                    <li class="nav-item list-group-item list-group-item-action @ViewData["ActiveHomeLeftMenu"] leftmenu p-0">
                        <a href="#" class="nav-link text-truncate" onclick="RedirectToOTMSHome()">
                            <i class="fs-5 bi-house"></i><span class="ms-1 d-sm-inline">Home</span>
                        </a>
                    </li>
                    <li class="list-group-item list-group-item-action @ViewData["Active" + @ViewData["LoggedInType"] + "Dashboard" +  "LeftMenu"] leftmenu p-0">
                        <a href="#submenu1" data-bs-toggle="collapse" class="nav-link text-truncate">
                            <i class="fs-5 bi-speedometer2"></i><span class="ms-1 d-sm-inline">@ViewData["LoggedInType"] Dashboard</span>
                        </a>
                    </li>
                    @{
                        if (ViewData["LoggedInType"]!.ToString() == "Admin")
                        {
                            <li class="list-group-item list-group-item-action @ViewData["ActiveInstructorDashboardLeftMenu"] leftmenu p-0">
                                <a href="#" class="nav-link text-truncate">
                                    <i class="fs-5 bi-table"></i><span class="ms-1 d-sm-inline">Instructor Dashboard</span>
                                </a>
                            </li>
                        }
                    }




                </ul>
            </div>
        </div>


        <div class="">
            <div class="card-header bg-light bg-gradient">
                <button class="btn" style="padding: revert !important;" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasScrolling" aria-controls="offcanvasScrolling">
                    <i class="fa-solid fa-bars"></i>
                </button>
            </div>

            <div class="pb-2">

                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>

    </div>
    


    <footer class="bg-white align-middle border-top footer text-muted w-100 start-0 bottom-0 text-wrap text-center">
